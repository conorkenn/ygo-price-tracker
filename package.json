{
  "name": "ygo-price-tracker",
  "version": "1.0.0",
  "description": "Track Yu-Gi-Oh! card prices on eBay and get Discord alerts",
  "main": "dist/check.js",
  "scripts": {
    "check": "node dist/check.js",
    "config:add": "node dist/config.js add",
    "config:remove": "node dist/config.js remove",
    "config:list": "node dist/config.js list",
    "prices:list": "node dist/prices.js list",
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "yugioh",
    "ebay",
    "price-tracker",
    "discord"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.3",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}
